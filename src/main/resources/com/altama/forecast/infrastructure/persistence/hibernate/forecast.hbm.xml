<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class mutable="true" name="com.altama.forecast.domain.forecast.Forecast" table="FORECAST">
        <id column="id" name="id">
            <generator class="org.hibernate.id.IdentityGenerator"/>
        </id>
        
        <property column="brandID" name="brandID" not-null="false"/>
        <property column="brand" name="brand" not-null="false"/>
        <property column="product" name="product" not-null="false"/>
        <property column="suplier" name="suplier" not-null="false"/>
        <property column="factory" name="factory" not-null="false"/>
        <property column="isDiscontinued" name="isDiscontinued" not-null="false">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.altama.forecast.domain.forecast.IsDiscontinued</param>
                <param name="type">12</param>
            </type>
        </property>
        
        <property column="priceVersion" name="priceVersion" not-null="false"/>
        <property column="qtyAvalilable" name="qtyAvalilable" not-null="false"/>
        <property column="qtyAverage" name="qtyAverage" not-null="false"/> 
        <property column="leadTime" name="leadTime" not-null="false"/>    
        <property column="qtyBatasAtas" name="qtyBatasAtas" not-null="false"/>
        <property column="qtyOnOrdered" name="qtyOnOrdered" not-null="false"/>
        <property column="priceListJual" name="priceListJual" not-null="false"/>
        <property column="currencyLastPCS" name="currencyLastPCS" not-null="false"/>
        <property column="priceListLastPCS" name="priceListLastPCS" not-null="false"/>
        <property column="moq" name="moq" not-null="false"/>
        <property column="qtyPCSperMasterBox" name="qtyPCSperMasterBox" not-null="false"/>
        <property column="rekomendasiQtyPO" name="rekomendasiQtyPO" not-null="false"/>
        <property column="totalMasterBox" name="totalMasterBox" not-null="false"/>
    
    </class>
</hibernate-mapping>
